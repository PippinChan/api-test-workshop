{
  "type": "object",
  "required": [
    "id",
    "stops",
    "drivingDistancesInMeters",
    "fare",
    "status",
    "createdAt"
  ],
  "properties": {
    "id": {
      "type": "number",
      "minimum": 1,
      "multipleOf": 1
    },
    "stops": {
      "type": "array",
      "minItems": 2,
      "items": {
        "type": "object",
        "required": [
          "lat",
          "lng"
        ]
      }
    },
    "drivingDistancesInMeters": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "number",
        "minimum": 1,
        "multipleOf": 1
      }
    },
    "fare": {
      "type": "object",
      "required": [
        "amount",
        "currency"
      ],
      "properties": {
        "amount": {
          "type": "string",
          "pattern": "[0-9]+\\.[0-9]+"
        },
        "currency": {
          "type": "string",
          "pattern": "[A-Z]{3}"
        }
      }
    },
    "status": {
      "type": "string",
      "pattern": "ASSIGNING|ONGOING|COMPLETED|CANCELLED"
    },
    "createdAt": {
      "type": "string",
      "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}\\.[0-9]{3}Z"
    },
    "orderAt": {
      "type": "string",
      "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}\\.[0-9]{3}Z",
      "optional": true
    }
  }
}
