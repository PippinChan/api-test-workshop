{
  "type": "object",
  "required": [
    "id",
    "status",
    "cancelledAt"
  ],
  "properties": {
    "id": {
      "type": "number",
      "minimum": 1,
      "multipleOf": 1
    },
    "status": {
      "type": "string",
      "pattern": "ASSIGNING|ONGOING|COMPLETED|CANCELLED"
    },
    "cancelledAt": {
      "type": "string",
      "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}\\.[0-9]{3,7}Z"
    }
  }
}
